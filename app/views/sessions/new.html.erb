<div class='auth-form-container'>
  <%=form_tag session_path, method: :post, class: 'auth-form' do %>
    <%= label_tag :email%>
    <%= text_field_tag :email %>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
    <%= submit_tag 'Sign In', class: 'auth-form-action' %>
  <% end %>
</div>
